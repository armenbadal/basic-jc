
package basic.ast;

public class For extends Statement {
	public Variable param;
	public Expression from;
	public Expression to;
	public Number step;
	public Statement body;

	public For( Variable p, Expression f, Expression t, Number s, Statement b )
	{
		param = p;
		from = f;
		to = t;
		step = s;
		body = b;
	}
}

