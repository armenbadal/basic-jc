'
' տեքստերի համեմատման գործողություններ
'

SUB eq$(x, y)
    LET eq$ = x = y
END SUB

SUB ne$(x, y)
    LET ne$ = x <> y
END SUB

SUB gt$(x, y)
    LET gt$ = x > y
END SUB

SUB ge$(x, y)
    LET ge$ = x >= y
END SUB

SUB lt$(x, y)
    LET lt$ = x < y
END SUB

SUB le$(x, y)
    LET le$ = x <= y
END SUB
